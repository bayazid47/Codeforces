#include <bits/stdc++.h>
using namespace std;

int main(){
  int n;
  cin>>n;
  string str;
  cin>>str;
  int Anton=0;
  int Danik=0;
  for(int i=0;i<n;i++){
    
      if(str[i]=='D'){
        Danik++;
      }
      else if(str[i]=='A'){
        Anton++;
      }
    }
    if(Anton>Danik){
      cout<<"Anton"<<endl;
    }
    else if(Anton==Danik){
      cout<<"Friendship"<<endl;
    }
    else{
      cout<<"Danik"<<endl;
    }
  return 0;
}
