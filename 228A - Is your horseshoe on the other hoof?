#include<bits/stdc++.h>
using namespace std;

int main(){
    int arr[4];
    int count=0;
    for(int i=0;i<4;i++){
        cin>>arr[i];
    }
    sort(arr,arr+4); //If the inputs aren't sorted at first make it sort.
    for(int i=0;i<4;i++){
        if(arr[i]==arr[i+1]){
            count++;
        }
        
    }
    cout<<count<<endl;
    return 0;
}
