#include <bits/stdc++.h>
#define fast() ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0)
#define endl "\n"
#define lli long long int
using namespace std;

void solve() {
    int l, r;
      cin >> l >> r;
    if( l == 1 && r == 1){
        cout << 1 << endl;
        return;
    }
    cout << r - l << endl;
}

signed main() {
    fast();
    lli t;
    cin >> t;
    while (t--) {
        solve();
    }
    return 0;
}
