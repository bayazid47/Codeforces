#include <iostream>
using namespace std;

int main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int a=n/3;
        int b=n%3;
        if(b==0){
            cout<<a<<" "<<a+1<<" "<<a-1<<endl;
        }
        else if(b==1){
            cout<<a<<" "<<a+2<<" "<<a-1<<endl;
        }
        else{
            cout<<a+1<<" "<<a+2<<" "<<a-1<<endl;
        }
    }
    return 0;
}
